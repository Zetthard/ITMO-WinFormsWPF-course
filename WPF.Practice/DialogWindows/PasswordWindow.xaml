<Window x:Class="DialogWindows.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DialogWindows"
        mc:Ignorable="d"
        Title="PasswordWindow" Height="150" Width="300">
    <Grid>
        <PasswordBox x:Name="MyPasswordBox" HorizontalAlignment="Center" Margin="0,28,0,0" VerticalAlignment="Top" Height="28" Width="215"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Height="22" Margin="184,71,0,0" VerticalAlignment="Top" Width="71" IsCancel="True"/>
        <Button Content="Ok" HorizontalAlignment="Left" Height="22" Margin="101,71,0,0" VerticalAlignment="Top" Width="71" IsDefault="True" Click="Button_Click"/>

    </Grid>
</Window>
