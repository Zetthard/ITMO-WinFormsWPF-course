<Window x:Class="WPFCalculator.WindowEqAnswer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFCalculator"
        mc:Ignorable="d"
        Title="WindowEqAnswer" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="188*"/>
            <ColumnDefinition Width="107*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TextBlockRootP" HorizontalAlignment="Left" Height="30" Margin="45,14,0,0" VerticalAlignment="Top" Width="203" Grid.ColumnSpan="2"/>
        <TextBlock x:Name="TextBlockRootN" HorizontalAlignment="Left" Height="30" Margin="45,45,0,0" VerticalAlignment="Top" Width="203" Grid.ColumnSpan="2"/>
        <Button x:Name="ButtonOk" Grid.ColumnSpan="2" Content="Ok" HorizontalAlignment="Left" Height="23" Margin="103,88,0,0" VerticalAlignment="Top" Width="94" Click="ButtonOk_Click"/>

    </Grid>
</Window>
